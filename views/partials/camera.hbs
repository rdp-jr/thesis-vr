<a-gltf-model 
    src="#camera" 
    class="clickable"
    scale="0.5 0.5 0.5" 
    rotation="0 180 0"
    position="3.839 0.482 -2.533" 
    onclick="clickCamera()"
    animation__mouseenter="property: scale; to: 0.505 0.505 0.505; startEvents: mouseenter; dur: 500";
    animation__mouseleave="property: scale; to: 0.5 0.5 0.5; startEvents: mouseleave; dur: 500";>
</a-gltf-model>