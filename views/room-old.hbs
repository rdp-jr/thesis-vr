        <a-plane color="#DDDDDD" shader="flat" rotation="-90 0 0" scale="10 40 1"></a-plane>

        <a-box color="#00AAFF" position="-3 0.5 -16" class="clickable"></a-box>

        <a-image src="#footsteps" position="-3 2.5 -16"  blink-teleportation class="clickable"></a-image>

        <a-entity id="camRig" position="0 0 12">
			<!-- CAMERA -->
			<a-camera id="cam" wasd-controls-enabled="false">
			</a-camera>
